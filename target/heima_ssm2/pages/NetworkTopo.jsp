	<script type="text/javascript">
			function getSwitches() {
				var swList = new Array();
				$.ajax({
					url: "http://192.168.16.137 :8080/stats/switches",
					type: "GET",
					dataType: "JSON",
					success: function (data) {
						for (var i = 0; i < data.length; i++) {
							var sw = data[i];
							var html="<tr>"+

								"<td align='justify' id=\""+sw+"\"><button  type='button' class='tn bg-olive btn-xs' onclick='getSwInfo(\""+i+"\",\""+sw+"\")'>"+sw+"</button></td>"+
							"</tr>";
							swList.push(sw);
							$("#tableSw").append(html);
						}
					}
				});
			}
			window.onload=getSwitches;
	</script>
	<script type="text/javascript">
		function getSwInfo(i,sw){
			var dpid=sw;
			var index=dpid+i;
			var index1="#"+dpid+i+1;
			var index2="#"+dpid+i+2;
			var index3="#"+dpid+i+3;
			var dataList=new Array();
			$.ajax({
				url:"http://192.168.16.137 :8080/stats/aggregateflow/"+dpid,
				datatype:"JSON",
				type:"GET",
				success:function(data){
					var pkt_count=data[dpid][0]["packet_count"];
					var byte_count=data[dpid][0]["byte_count"];
					var flow_count=data[dpid][0]["flow_count"];
					var html="<td id=\""+index1+"\">"+pkt_count+"</td>"+
							"<td id=\""+index2+"\">"+Math.round(byte_count/1024)+"</td>"+
							"<td id=\""+index3+"\">"+flow_count+"</td>"+
							"<td class='text-center'><button type='button' class='btn bg-primary btn-xs'>详情</button></td>";
					var newNode=document.getElementById(index1);
					if(!newNode){
						var pd="#"+dpid;
						$(html).insertAfter(pd);
					}else{
						document.getElementById(index1).innerText=pkt_count;
						document.getElementById(index2).innerText=byte_count;
						document.getElementById(index3).innerText=flow_count;
					}
					dataList["pkt_count"]=pkt_count;
					dataList["byte_count"]=byte_count;
					dataList["flow_count"]=flow_count;
				}
			});
		}
	</script>



